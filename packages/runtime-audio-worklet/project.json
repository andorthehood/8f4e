{
  "name": "@8f4e/runtime-audio-worklet",
  "sourceRoot": "src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "commands": [
          "vite build -c vite.config.worklet.ts",
          "tsc"
        ],
        "cwd": "{projectRoot}",
        "parallel": false
      },
      "dependsOn": ["^build"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint src --fix",
        "cwd": "{projectRoot}"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite build -c vite.config.worklet.ts --watch",
        "cwd": "{projectRoot}"
      }
    },
    "test": {
      "executor": "@nx/vitest:test",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "config": "{projectRoot}/vitest.config.ts",
        "reportsDirectory": "{workspaceRoot}/coverage/{projectRoot}"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "{projectRoot}"
      },
      "dependsOn": ["^build"]
    }
  },
  "tags": ["type:runtime", "scope:audio"]
}

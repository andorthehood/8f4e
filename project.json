{
  "name": "app",
  "sourceRoot": "src",
  "projectType": "application",
  "implicitDependencies": [
    "@8f4e/compiler-worker",
    "@8f4e/runtime-web-worker-logic",
    "@8f4e/runtime-web-worker-midi",
    "@8f4e/runtime-audio-worklet"
  ],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx run-many --target=build --all && NX_DAEMON=true nx watch --all -- nx run-many --target=build --all",
          "vite"
        ],
        "parallel": true,
        "cwd": "{workspaceRoot}"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint src --fix",
        "cwd": "{workspaceRoot}"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist"],
      "options": {
        "command": "vite build",
        "cwd": "{workspaceRoot}"
      },
      "dependsOn": ["typecheck", "^build"]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite preview",
        "cwd": "{workspaceRoot}"
      },
      "dependsOn": ["build"]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit --project tsconfig.build.json",
        "cwd": "{workspaceRoot}"
      },
      "dependsOn": ["^build"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run --config vitest.config.ts",
        "cwd": "{workspaceRoot}"
      },
      "dependsOn": ["^build"]
    },
    "kill-dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "lsof -ti tcp:3000 | xargs -r kill",
        "cwd": "{workspaceRoot}"
      }
    }
  },
  "tags": ["type:application", "scope:root"]
}

{
  "name": "app",
  "sourceRoot": "src",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite",
        "cwd": "{workspaceRoot}"
      },
      "dependsOn": ["^build"]
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist"],
      "options": {
        "command": "vite build",
        "cwd": "{workspaceRoot}"
      },
      "dependsOn": ["^build"]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite preview",
        "cwd": "{workspaceRoot}"
      },
      "dependsOn": ["build"]
    }
  },
  "tags": ["type:application", "scope:root"]
}

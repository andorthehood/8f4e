{
	"name": "@8f4e/web-ui",
	"version": "1.0.0",
	"private": true,
	"description": "8f4e web UI package - canvas renderer for the editor",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"sideEffects": false,
	"scripts": {
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:screenshot": "playwright test",
		"test:screenshot:update": "playwright test --update-snapshots",
		"test:screenshot:ui": "playwright test --ui",
		"test:screenshot:headed": "playwright test --headed",
		"test:screenshot:debug": "playwright test --debug",
		"build": "tsc",
		"dev": "tsc --watch",
		"dev:test": "vite --config screenshot-tests/vite.config.ts",
		"typecheck": "tsc --noEmit"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/andorthehood/8f4e.git"
	},
	"author": "Andor Polgar",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/andorthehood/8f4e/issues"
	},
	"homepage": "https://github.com/andorthehood/8f4e#readme",
	"dependencies": {
		"glugglug": "*",
		"@8f4e/sprite-generator": "*",
		"@8f4e/editor-state": "*"
	},
	"devDependencies": {
		"vite": "^5.0.0"
	}
}

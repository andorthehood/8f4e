{
	"name": "@8f4e/editor-state",
	"version": "1.0.0",
	"private": true,
	"description": "8f4e editor state management package",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./testing": {
			"types": "./dist/testing.d.ts",
			"default": "./dist/testing.js"
		}
	},
	"sideEffects": false,
	"scripts": {
		"test": "vitest",
		"coverage": "vitest --coverage",
		"build": "tsc",
		"dev": "tsc --watch",
		"typecheck": "tsc --noEmit"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/andorthehood/8f4e.git"
	},
	"author": "Andor Polgar",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/andorthehood/8f4e/issues"
	},
	"homepage": "https://github.com/andorthehood/8f4e#readme",
	"dependencies": {
		"glugglug": "*",
		"@8f4e/sprite-generator": "*",
		"@8f4e/compiler": "*"
	},
	"devDependencies": {
		"@types/node": "^24.3.0",
		"vitest": "^3.2.4",
		"jest": "^29.7.0",
		"@swc/jest": "^0.2.39",
		"@types/jest": "^29.5.12"
	}
}

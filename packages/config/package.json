{
	"name": "@8f4e/config",
	"version": "1.0.0",
	"private": true,
	"description": "Shared tooling configuration for 8f4e workspace",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./ts": {
			"types": "./dist/ts/index.d.ts",
			"default": "./dist/ts/index.js"
		},
		"./vitest": {
			"types": "./dist/vitest/index.d.ts",
			"default": "./dist/vitest/index.js"
		},
		"./vite": {
			"types": "./dist/vite/index.d.ts",
			"default": "./dist/vite/index.js"
		},
		"./eslint": {
			"types": "./dist/eslint/index.d.ts",
			"default": "./dist/eslint/index.js"
		},
		"./tsconfig.base.json": "./tsconfig.base.json",
		"./tsconfig.node.json": "./tsconfig.node.json",
		"./tsconfig.dom.json": "./tsconfig.dom.json",
		"./tsconfig.webworker.json": "./tsconfig.webworker.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/andorthehood/8f4e.git"
	},
	"author": "Andor Polgar",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/andorthehood/8f4e/issues"
	},
	"homepage": "https://github.com/andorthehood/8f4e#readme",
	"peerDependencies": {
		"typescript": ">=5.0.0",
		"vite": ">=7.0.0",
		"vitest": ">=4.0.0"
	},
	"peerDependenciesMeta": {
		"vite": {
			"optional": true
		},
		"vitest": {
			"optional": true
		}
	}
}

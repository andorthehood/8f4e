{
	"name": "@8f4e/examples",
	"version": "1.0.0",
	"private": true,
	"description": "8f4e example modules and projects",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./modules/*": {
			"types": "./dist/modules/*.d.ts",
			"default": "./dist/modules/*.js"
		},
		"./projects/*": {
			"types": "./dist/projects/*.d.ts",
			"default": "./dist/projects/*.js"
		}
	},
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/andorthehood/8f4e.git"
	},
	"author": "Andor Polgar",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/andorthehood/8f4e/issues"
	},
	"homepage": "https://github.com/andorthehood/8f4e#readme",
	"dependencies": {
		"@8f4e/editor-state": "*"
	}
}

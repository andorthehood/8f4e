{
	"name": "@8f4e/syntax-rules",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/syntax-rules/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "nx:run-commands",
			"outputs": ["{projectRoot}/dist"],
			"options": {
				"command": "tsc",
				"cwd": "packages/syntax-rules"
			}
		},
		"test": {
			"executor": "nx:run-commands",
			"options": {
				"command": "vitest run",
				"cwd": "packages/syntax-rules"
			}
		},
		"typecheck": {
			"executor": "nx:run-commands",
			"options": {
				"command": "tsc --noEmit",
				"cwd": "packages/syntax-rules"
			}
		}
	}
}
